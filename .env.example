ADMIN_PASSWORD=admin

# Internal service ports
API_PORT=3000
USERS_PORT=3001
TOURNAMENTS_PORT=3002
FRONTEND_PORT=3003
PROXY_PORT=3004

# External application port
APP_PORT=443

VITE_ENABLE_DEV_PAGES=false
REPORT_GUEST_AS_ZERO=true

# Password for the 'elastic' user and 'kibana_system' user
# (at least 6 characters)
ELASTIC_PASSWORD=changeme
KIBANA_PASSWORD=changeme

# Version of Elastic products
STACK_VERSION=8.7.1
CLUSTER_NAME=transcendence-cluster

# Set to 'basic' or 'trial' to automatically start the 30-day trial
#LICENSE=basic
LICENSE=trial

# External elk stack ports
ES_PORT=9200
KIBANA_PORT=5601

# Memory limits for elk stack services (in bytes)
ES_MEM_LIMIT=1073741824
KB_MEM_LIMIT=1073741824
LS_MEM_LIMIT=1073741824

# Secrets
JWT_SECRET={generated-by-makefile}
INTERNAL_API_KEY={generated-by-makefile}
ENCRYPTION_KEY={generated-by-makefile}